{
  "name": "ts-express-mongodb-restapi-boilerplate",
  "version": "1.0.0",
  "description": "This is my hyper beast stack for creating rest api.",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon --watch './**/*.ts' --exec 'ts-node' src/server.ts",
    "start": "ts-node dist/server.js",
    "build": "rm -rf dist/ && prettier --write src/ && tsc"
  },
  "author": "Suraj Karki",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.18",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.32",
    "nodemon": "^2.0.7"
  }
}
